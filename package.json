{
	"name": "bakahttp",
	"version": "0.0.0",
	"description": "http test client",
	"license": "MIT",
	"repository": "mokafish/bakahttp",
	"funding": "https://github.com/mokafish/bakahttp",
	"author": {
		"name": "mokafish",
		"email": "mokafish@users.noreply.github.com",
		"url": "https://github.com/mokafish"
	},
	"type": "module",
	"bin": {
		"bakahttp": "./dist/cli.js"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"make": "babel --out-dir=dist src",
		"dev": "babel --out-dir=dist --watch src",
		"real-test": "prettier --check  --write  . && xo && ava",
		"test": "echo pass"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"cli-app",
		"cli",
		"http",
		"test",
		"net",
		"tool"
	],
	"dependencies": {
		"ink": "^5.2.0",
		"meow": "^13.2.0",
		"react": "^19.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.27.0",
		"@babel/preset-react": "^7.26.3",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^6.2.0",
		"chalk": "^5.4.1",
		"eslint-config-xo-react": "^0.28.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"import-jsx": "^5.0.0",
		"ink-testing-library": "^4.0.0",
		"prettier": "^3.5.3",
		"xo": "^0.60.0"
	},
	"ava": {
		"environmentVariables": {
			"NODE_NO_WARNINGS": "1"
		},
		"nodeArguments": [
			"--loader=import-jsx"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	}
}
